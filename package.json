{
  "name": "@ow3/eslint-config-monorepo",
  "version": "0.30.2",
  "private": true,
  "packageManager": "pnpm@7.13.3",
  "author": "Chris Breuer",
  "license": "MIT",
  "engines": {
    "node": ">=v16.15.1",
    "pnpm": ">=7.13.3"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "pnpm -r run stub",
    "fresh": "rimraf node_modules packages/**/node_modules pnpm-lock.yaml && pnpm i",
    "release": "bumpp package.json packages/*/package.json",
    "test": "pnpm -r run test"
  },
  "devDependencies": {
    "@ow3/eslint-config": "*",
    "bumpp": "^8.2.0",
    "eslint": "^8.16.0",
    "eslint-plugin-ow3": "workspace:*",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.2"
  }
}
