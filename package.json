{
  "name": "@ow3/eslint-config-monorepo",
  "version": "0.37.0",
  "private": true,
  "packageManager": "pnpm@7.21.0",
  "author": "Chris Breuer",
  "license": "MIT",
  "engines": {
    "node": ">=v16.15.1",
    "pnpm": ">=7.13.3"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "pnpm -r run stub",
    "fresh": "rimraf node_modules packages/**/node_modules pnpm-lock.yaml && pnpm i",
    "release": "bumpp -r && pnpm -r publish",
    "test": "pnpm -r run test"
  },
  "devDependencies": {
    "@ow3/eslint-config": "*",
    "bumpp": "^9.0.0",
    "eslint": "^8.34.0",
    "eslint-plugin-ow3": "workspace:*",
    "rimraf": "^4.1.2",
    "typescript": "^4.9.5"
  }
}
